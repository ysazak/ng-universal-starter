    "jasmine-spec-reporter": "4.1.0",
    "lite-server": "2.3.0",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.18.2",
    "to-string-loader": "1.1.5",
    "typescript": "2.3.4",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  }

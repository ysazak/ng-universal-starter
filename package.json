{
  "name": "ng-deploy-sample",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server.js",
    "clean:dist": "rimraf dist",
    "clean:ngc": "rimraf **/*.ngfactory.ts **/*.css.shim.ts",
    "prebuild": "npm run clean:dist",
    "build": "ng build",
    "build:aot": "webpack --config webpack.config.aot.js",
    "build:uni": "webpack --config webpack.config.uni.js",
    "ngc": "ngc -p tsconfig.aot.json",
    "serve": "lite-server -c=bs-config.json",
    "serve:aot": "lite-server -c=bs-config.aot.js",
    "serve:uni": "node dist/server.js",
    "update-deps": "rm -rf ./node_modules && npm install",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "6.9.2",
    "npm": "3.10.9"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.3",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.1.3",
    "@angular/upgrade": "4.1.3",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "jquery": "3.2.1",
    "rxjs": "5.4.0",
    "typings": "^2.1.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.1",
    "@angular/compiler-cli": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@ngtools/webpack": "1.4.1",
    "@types/core-js": "0.9.41",
    "@types/jquery": "2.0.46",
    "@types/node": "7.0.29",
    "chokidar": "1.7.0",
    "concurrently": "3.4.0",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
